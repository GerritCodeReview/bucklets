[access]
	inheritFrom = Public-Projects
[project]
	description = Reusable building blocks for Buck build system.
[label "Library-Compliance"]
	function = MaxWithBlock
	copyAllScoresIfNoCodeChange = true
	copyAllScoresOnTrivialRebase = true
	value = -1 Do not submit
	value =  0 No score
	value = +1 Approved
	defaultValue = 0
[access "refs/heads/*"]
	label-Code-Review = -2..+2 group bucklets-maintainers
	label-Code-Review = -2..+2 group google/gerritcodereview-maintainers@googlegroups.com
	label-Verified = -1..+1 group bucklets-maintainers
	label-Verified = -1..+1 group gerrit-verifiers
	submit = group bucklets-maintainers
	submit = group google/gerritcodereview-maintainers@googlegroups.com
	abandon = group bucklets-maintainers
	abandon = group gerrit-verifiers
	removeReviewer = group bucklets-maintainers
	removeReviewer = group google/gerritcodereview-maintainers@googlegroups.com
	editTopicName = group bucklets-maintainers
	editTopicName = +force group google/gerritcodereview-maintainers@googlegroups.com
	label-Library-Compliance = -1..+1 group gerrit-lib
[access "refs/*"]
	owner = group bucklets-maintainers
	owner = group google/gerritcodereview-maintainers@googlegroups.com
[submit-requirement "Library-Compliance"]
	description = Submit requirement for the 'Library-Compliance' label
	submittableIf = label:Library-Compliance=MAX -label:Library-Compliance=MIN
	canOverrideInChildProjects = true
